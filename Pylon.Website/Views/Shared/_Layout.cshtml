<body class="loader-active">

	<div class="content">
		<!--== Preloader Area Start ==-->
		<div class="preloader">
			<div class="preloader-spinner">
				<div class="loader-content">
					<img src="/assets/img/preloader.gif" alt="JSOFT">
				</div>
			</div>
		</div>
		<!--== Preloader Area End ==-->
		<!--== Header Area Start ==-->
		<header id="header-area" class="fixed-top">
			<!--== Header Bottom Start ==-->
			<div id="header-bottom">
				<div class="container">
					<div class="row">
						<div class="col-lg-2">
							<div class="div-new-image">
								<img src="/assets/img/car-logo.png">
								<img src="/assets/img/pylon.png">
							</div>
						</div>
						<!--== Main Menu Start ==-->
						<div class="col-lg-8 d-none d-xl-block">
							<nav class="mainmenu alignright">
								<ul>
									@if (User.IsInRole("admin"))
									{
										<li>@Html.ActionLink(@Resources.Resource.Admin, "GetAllUsers", "Admin") </li>
									}
									@if (User.IsInRole("customer") || User.IsInRole("saler"))
									{
										<li>@Html.ActionLink(@Resources.Resource.AllCars, "GetAll", "Product") </li>
									}
									@if (User.IsInRole("saler"))
									{
										<li>@Html.ActionLink(@Resources.Resource.MyCars, "GetSalerProducts", "Product") </li>
									}
									@if (User.IsInRole("customer") || User.IsInRole("saler"))
									{
										<li>@Html.ActionLink(@Resources.Resource.Profile, "GetInfo", "Profile") </li>
									}
									@if (User.IsInRole("customer"))
									{
										<li>@Html.ActionLink(@Resources.Resource.MyOrders, "GetCustomerOrders", "Order") </li>
									}
									<li>@Html.Partial("_LoginPartial")</li>
								</ul>
								<div>
									@using (Html.BeginForm("ChangeCulture", "Language"))
									{
										<select name="lang">
											<option disabled selected>@Resources.Resource.ChooseLanguage</option>
											<option value="ru">Русский</option>
											<option value="en">English</option>
										</select>
										<input type="submit" name="submit" value="@Resources.Resource.ChooseLanguage">
									}
								</div>
							</nav>
						</div>
						<!--== Main Menu End ==-->
					</div>
				</div>
			</div>
			<!--== Header Bottom End ==-->
		</header>
		<!--== Header Area End ==-->
		<!--== Page Title Area Start ==-->
		<section id="page-title-area" class="section-padding overlay">
			<div class="container">
				<div class="row">
				</div>
			</div>
		</section>


		<div class="container body-content">
